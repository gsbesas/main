<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="report_producttemplatelabel_gsbesas">
            <t t-call="web.basic_layout">
                <div class="page">
                    <t t-foreach="docs" t-as="template">
                        <t t-foreach="template.product_variant_ids" t-as="product">
                            <t t-call="gsbesas_product_label.report_simple_label_gsbesas">
                                <t t-set="product" t-value="product"/>
                            </t>
                        </t>
                    </t>
                </div>
            </t>
        </template>
        
        <template id="report_producttemplatelabel_gsbesas_medium">
            <t t-call="web.basic_layout">
                <div class="page">
                    <t t-foreach="docs" t-as="template">
                        <t t-foreach="template.product_variant_ids" t-as="product">
                            <t t-call="gsbesas_product_label.report_simple_label_gsbesas_medium">
                                <t t-set="product" t-value="product"/>
                            </t>
                        </t>
                    </t>
                </div>
            </t>
        </template>

    </data>
</odoo>
