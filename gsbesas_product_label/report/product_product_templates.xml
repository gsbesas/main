<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="report_simple_label_gsbesas">
            <div style="width: 100%;">
                <!--<table cellpadding="0" cellspacing="0" style="border: 1px solid black; width: 6cm; margin-left: -10px; height: 44px;">-->
                <table style="border: 1px solid black; width: 6cm; margin-left: -10px;">
                    <tr>
                        <td class="text-center" style="line-height: 100%;">
                            <!--<strong style="font-size:10pt;" t-field="product.display_name"/>-->
                            <p>
                                <strong>
                                    <span style="font-size:10pt;" t-if="product.display_name" t-esc="str(product.display_name)[:54]"/>
                                </strong>
                            </p>
                        </td>
                    </tr>
                </table>
                <!--<table cellpadding="0" cellspacing="0" style="border: 1px solid black; width: 6cm; border-top: none; margin-left: -10px; height: 74px;">-->
                <table style="border: 1px solid black; width: 6cm; border-top: none; margin-left: -10px; height: 65px;">
                    <tr>
                        <td class="text-center align-middle" style="width: 2.5cm; border-right: 1px solid;">
                            <t t-if="product.fnsku_usa_barcode">
                                <img alt="Barcode" t-if="len(product.fnsku_usa_barcode) == 13" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('EAN13', quote_plus(product.barcode or ''), 600, 120)" style="width:100%;"/>
                                <img alt="Barcode" t-elif="len(product.fnsku_usa_barcode) == 8" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('EAN8', quote_plus(product.barcode or ''), 600, 120)" style="width:100%;"/>
                                <img alt="Barcode" t-else="" t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', quote_plus(product.fnsku_usa_barcode or ''), 600, 120)" style="width:100%;"/>
                                <span style="font-size:9pt" t-field="product.fnsku_usa_barcode"/>&#160;&#160;
                                <span class="text-left" style="font-size:9pt; ">- NEW</span>
                            </t>
                            <t t-else=""><span class="text-muted">No barcode available</span></t>
                        </td>
                        <td class="text-center align-middle" style="width: 1cm">
                            <strong><span style="font-size:9pt; font-weight: bold; vertical-align: top;">EXPIRY: </span>
                            <span t-field="product.exp_date" t-options='{"format": "dd-MMM-yyyy"}' style="font-size:8pt; font-weight: bold; vertical-align: top;"/>
                            </strong>
                        </td>
                    </tr>
                </table>
                <table cellpadding="0" cellspacing="0" style="border: 1px solid black; width: 6cm; border-top: none; margin-left: -10px; background-color:#2F4F4F;">
                    <tr>
                        <td class="text-center">
                            <strong><span style="font-size:10pt; color:white;">ALLERGENS:</span><span t-field="product.allergens" style="font-size:10pt; color:white;"/></strong>
                        </td>
                    </tr>
                </table>
            </div>
        </template>
        
        <template id="report_productlabel_gsbesas">
            <t t-call="web.basic_layout">
                <div class="page">
                    <t t-foreach="docs" t-as="product">
                        <t t-call="gsbesas_product_label.report_simple_label_gsbesas">
                            <t t-set="product" t-value="product"/>
                        </t>
                    </t>
                </div>
            </t>
        </template>
        
